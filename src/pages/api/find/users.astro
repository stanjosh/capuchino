---
import UserTable from '../../admin/partials/UserTable.astro'
import db from '../../../../lib/db'
const formData = await Astro.request.formData()
const searchTerm = formData.get('search')?.toString()


const users = searchTerm ? await db.user.search( searchTerm as string ) : await db.user.getAll()
---
<UserTable data={users} />


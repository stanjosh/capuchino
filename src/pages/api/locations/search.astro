---
import LocationTable from '../../admin/partials/LocationTable.astro'
import db from '../../../../lib/db'
const formData = await Astro.request.formData()
const searchTerm = formData.get('search')?.toString()


const locations = searchTerm ? await db.location.search( searchTerm as string ) : await db.location.getAll()
---
<LocationTable data={locations} />


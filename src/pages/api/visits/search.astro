---
import VisitTable from '../../admin/partials/VisitTable.astro'
import db from '../../../../lib/db'
const formData = await Astro.request.formData()
const searchTerm = formData.get('search')?.toString()


const visits = searchTerm ? await db.visit.search( searchTerm as string ) : await db.visit.getAll()
---
<VisitTable data={visits} />

